<?xml version="1.0" ?>
<robot name="hand_transmission" xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:macro name="hand_transmission" params="prefix_hand prefix_finger">

    <effort_joint_transmission joint="${prefix_hand}_${prefix_finger}_l_joint" reduction="1" />
    <effort_joint_transmission joint="${prefix_hand}_${prefix_finger}_r_joint" reduction="1" />

  </xacro:macro>
</robot>
