    <xacro:velocity_joint_transmission joint="${prefix}_l_drive_wheel_joint" reduction="1" />
    <xacro:velocity_joint_transmission joint="${prefix}_r_drive_wheel_joint" reduction="1" />

  </xacro:macro>
</robot>
